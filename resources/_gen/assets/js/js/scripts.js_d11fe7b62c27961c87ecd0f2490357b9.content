window.addEventListener('load',function(){startTimer();startCounter();addInfiniscroll();});function startTimer=function(){if(document.getElementById('timer')){var timer=document.getElementById('timer');setInterval(function(){function pad(n){return String("00"+n).slice(-2);}
var date=new Date();timer.innerHTML=pad(date.getHours())+'<span>:</span>'+pad(date.getMinutes())+'<span>:</span>'+pad(date.getSeconds());},1000,timer);}}